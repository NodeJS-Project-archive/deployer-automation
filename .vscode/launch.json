{
    "version": "0.2.0",
    "configurations": [
         {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}\\home.js",
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Port",
            "port": 5858,
            "address":"localhost",
            "restart":true,
            "sourceMaps":false,
            "localRoot": "${workspaceRoot}",
            "remoteRoot":null
        }

    ]
}